<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE configuration PUBLIC "-//mybatis.org//DTD Config 3.0//EN" "http://mybatis.org/dtd/mybatis-3-config.dtd">
<configuration>
	<settings>
		<!-- 缓存 -->
		<setting name="cacheEnabled" value="true" />
        <!-- 支持java驼峰命名与DB下划线映射 -->
        <setting name="mapUnderscoreToCamelCase" value="true" />
	</settings>

	<!--Begin:add by Wenlong Meng at 2016-01-10 22:39 for 数据源重构 -->
	<plugins>
		<plugin interceptor="dragon.framework.datasource.DefaultDataSourceInterceptor"></plugin>
	</plugins>
	<!--End:add by Wenlong Meng at 2016-01-10 22:39 for 数据源重构 -->

</configuration>
